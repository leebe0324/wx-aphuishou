.unusedCoupons{
    padding: 36rpx 36rpx 0 36rpx;
}
.unusedCoupons > view{
    background-image: linear-gradient(-179deg, #FF9E7B 0%, #F33E65 100%);
    box-shadow: 0 3px 10px 0 rgba(192,63,63,0.27);
    border-radius: 14rpx;
    display: flex;
    padding: 0 31rpx 25rpx 31rpx;
    color: #ffffff;
    position: relative;
}
.w50{
    width: 50%;
}
.yuan{
    font-size: 48rpx;
    position: absolute;
    top: 60rpx;
    left: 32rpx;
}

.unusedCoupons > view > view:nth-child(2) text{
    display: block;
    font-size: 24rpx;
    padding: 6rpx 0;
}

.unusedCoupons > view > view:nth-child(2){
    padding-top: 51rpx;
}

.unusedCoupons > view > view .fsize{
    font-size: 200rpx;
    padding-left: 50rpx;
}

button{
    background: #FFFFFF;
    border-radius: 54px;
    font-size: 26rpx;
    color: #f37a3f;
    line-height: 60rpx;
    margin-top: 10rpx;
}

.textc {
    margin-top: 40%;
    text-align: center;
    color: #999;
}
